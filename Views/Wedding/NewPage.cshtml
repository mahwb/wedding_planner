    <a href="/logout">Logout</a>
    <a href="/dashboard">Dashboard</a>
    <h1>Plan your Wedding</h1>
</nav>
@model wedding_planner.Models.WeddingCheck

<form asp-controller="Wedding" asp-action="New" method="POST">
    <label asp-for="WedderOne"></label>
    <input asp-for="WedderOne" class="form-control">
    <span asp-validation-for="WedderOne"></span><br>
    <label asp-for="WedderTwo"></label>
    <input asp-for="WedderTwo" class="form-control">
    <span asp-validation-for="WedderTwo"></span><br>
    <label asp-for="Date"></label>
    <input asp-for="Date" type="date" class="form-control">
    <span asp-validation-for="Date"></span><br>
    <label asp-for="Address"></label>
    <input asp-for="Address" class="form-control">
    <span asp-validation-for="Address"></span><br>
    <button type="submit" name="submit" class="btn btn-primary">Submit</button>
</form>
